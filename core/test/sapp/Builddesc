INSTALL(regress/sapp/conf
	srcs[
		custom-acl.conf.in
		default-acl.conf.in
		sapp-tests.conf.in
		invalid-cert.conf.in
		log.conf.in
		log-all.conf.in
	]
	conf[
		custom-acl.conf
		default-acl.conf
		sapp-tests.conf
		invalid-cert.conf
		log.conf
		log-all.conf
		#ca.key.pem
		#ca.cert.pem
	]
	specialsrcs[
		#gencacert::ca.key.pem:certfile=ca.cert.pem
		#phony:ca.key.pem:ca.cert.pem
	]
)

COMPONENT([sapp-test-server sapp-tests invalid-cert])
